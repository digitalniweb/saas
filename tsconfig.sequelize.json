{
	"compilerOptions": {
		"target": "ES2022",
		"module": "CommonJS",
		"moduleResolution": "Node",
		"types": [],
		"outDir": "./sequelize",
		"rootDir": "./",
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"strict": true,
		"typeRoots": ["./types", "./digitalniweb-types"],
		"strictNullChecks": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"include": [
		"./server/migrations",
		"./server/seeders",
		"./server/config/config.ts"
	]
}
