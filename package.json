{
	"private": true,
	"type": "module",
	"packageManager": "npm@10.9.2",
	"scripts": {
		"build": "nuxt build",
		"dev": "nuxt dev",
		"start": "nuxt start",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"buildSequelize": "npx rimraf --glob sequelize/server/ && npx tsc --project tsconfig.sequelize.json && node -e \"require('fs').writeFileSync('./sequelize/package.json', JSON.stringify({type: 'commonjs'}))\"",
		"migrations": "npm run buildSequelize && npx sequelize-cli db:migrate",
		"migrationsUndoAll": "npm run buildSequelize && npx sequelize-cli db:migrate:undo:all",
		"seeders": "npm run buildSequelize && npx sequelize-cli db:seed:all"
	},
	"devDependencies": {
		"@nuxt/types": "^2.18.1",
		"@types/codemirror": "^5.60.15",
